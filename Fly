-- Load the player's GUI from ReplicatedStorage without adding fly code
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer or Players.PlayerAdded:Wait()

-- Wait for your GUI (FlyGUI) to exist
local gui = ReplicatedStorage:WaitForChild("FlyGUI")

-- Clone it into PlayerGui
local clonedGui = gui:Clone()
clonedGui.Parent = player:WaitForChild("PlayerGui")
